<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<act_window id="action_image_test" name="Image" res_model="nn.image.test"
			view_mode="tree,form" />
		<menuitem id="menu_shipment_management_image_test" action="action_image_test"
			parent="menu_shipment_management_administrative_root" sequence="20" />

		<record id="view_images_test_form" model="ir.ui.view">
			<field name="name">nn.image.test.form</field>
			<field name="model">nn.image.test</field>
			<field name="arch" type="xml">
				<form string="Our images">
					<group>
						<field name="name" />
						<field name="image" widget="image" />
					</group>
				</form>
			</field>
		</record>
		<record id="view_images_test_tree" model="ir.ui.view">
      		<field name = "name">nn.image.test.tree</field>
    		<field name = "model">nn.image.test</field>
    		<field name = "arch" type = "xml">
    			<tree>
    				<field name="name" />
    			</tree>
    		</field>
      	</record>
		
	</data>
</openerp>
	      		